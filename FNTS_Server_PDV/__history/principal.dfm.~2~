      Size = 4
    end
    object qrItemSubVALOR_DESCONTO: TFloatField
      FieldName = 'VALOR_DESCONTO'
      ReadOnly = True
    end
    object qrItemSubVALOR_ACRESCIMO: TFloatField
      FieldName = 'VALOR_ACRESCIMO'
      ReadOnly = True
    end
    object qrItemSubVALOR_UNITARIO: TFloatField
      FieldName = 'VALOR_UNITARIO'
      ReadOnly = True
    end
    object qrItemSubVALOR_TOTAL: TFloatField
      FieldName = 'VALOR_TOTAL'
      ReadOnly = True
    end
    object qrItemSubCST: TStringField
      FieldName = 'CST'
      ReadOnly = True
      Required = True
      Size = 3
    end
  end
end
