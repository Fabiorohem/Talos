      '  round(ite.VALOR_UNITARIO * sub.qtde,2) VALOR_TOTAL,'
      '  ite.cst'
      'from'
      '  cupom_item_sub sub'
      '  inner join cupom_item ite on ite.codigo = sub.cod_item'
      
